{
  "name": "@bluejay/collection",
  "version": "4.2.6",
  "description": "Convenient array subclass",
  "main": "dist/index.js",
  "typings": "dist",
  "scripts": {
    "test": "mocha --require ./node_modules/ts-node/register --require ./test/setup.ts ./test/**/*.test.ts",
    "cov": "./node_modules/.bin/nyc --reporter lcov npm run test && ./node_modules/.bin/opn ./coverage/lcov-report/collection/src/classes/collection.ts.html",
    "lint": "./node_modules/.bin/tslint --project ./",
    "fix": "./node_modules/.bin/tslint --fix --project ./",
    "compile": "./node_modules/.bin/tsc",
    "build": "rm -rf dist && mkdir dist && ./node_modules/.bin/tsc -p tsconfig.build.json -d",
    "prepare": "npm test && npm run lint && npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "author": "Sylvain Estevez",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluebirds-blue-jay/collection"
  },
  "license": "MIT",
  "dependencies": {
    "@bluejay/utils": "3.4.1",
    "lodash": "4.17.10"
  },
  "devDependencies": {
    "@types/chai": "4.0.4",
    "@types/lodash": "4.14.78",
    "@types/mocha": "2.2.43",
    "@types/node": "8.0.46",
    "chai": "4.1.2",
    "mocha": "4.0.1",
    "nyc": "13.0.0",
    "opn-cli": "3.1.0",
    "ts-node": "4.1.0",
    "tslint": "5.10.0",
    "typedoc": "0.11.1",
    "typescript": "2.5.3"
  }
}
